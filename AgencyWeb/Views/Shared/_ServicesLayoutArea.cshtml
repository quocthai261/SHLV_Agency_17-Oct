<!DOCTYPE html>

<html>
<head>
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="description" content="">
	<meta name="author" content="">
	<title>FPT - Agency Portal</title>

	<link rel="preconnect" href="https://fonts.gstatic.com">
	<link rel="shortcut icon" href="~/add_static//img/icons/icon-48x48.png" />
	<link rel="canonical" href="https://demo-basic.adminkit.io/" />
	<link href="~/add_static//css//app.css" rel="stylesheet">

	<!-- CSS -->
	<link href="~/css/add_main/normalize.css" rel="stylesheet" />
	<link href="~/css/add_main/animate.css" rel="stylesheet" />
	<link href="~/css/add_main/stellarnav.min.css" rel="stylesheet" />
	<link href="~/css/add_main/owl.carousel.css" rel="stylesheet" />
	<link href="~/css/add_main/owl.theme.default.min.css" rel="stylesheet" />
	<link href="~/css/add_main/bootstrap.min.css" rel="stylesheet" />
	<link href="~/css/add_main/font-awesome.min.css" rel="stylesheet" />
	<link href="~/css/add_main/style.css" asp-append-version="true" rel="stylesheet" />
	<link href="~/css/add_main/responsive.css" asp-append-version="true" rel="stylesheet" />
	<link href="~/css/add_main/common.css" asp-append-version="true" rel="stylesheet" />
	<link href="~/css/devextreme/dx.common.css" rel="stylesheet" />
	<link href="~/css/devextreme/dx.light.css" rel="stylesheet" />
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
	<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" />
	<link href="~/css/site.css" rel="stylesheet" />

	<link rel="stylesheet" type="text/css" href="https://pixinvent.com/stack-responsive-bootstrap-4-admin-template/app-assets/fonts/simple-line-icons/style.min.css">
	<link rel="stylesheet" type="text/css" href="https://pixinvent.com/stack-responsive-bootstrap-4-admin-template/app-assets/css/colors.min.css">

	<!-- JS -->
	<script src="~/js/add_main/modernizr.min.js"></script>
	<script src="~/js/add_main/jquery.min.js"></script>
	<script src="~/js/add_main/bootstrap.min.js"></script>
	<script src="~/js/devextreme/dx.all.js"></script>
	<script src="~/js/devextreme/exceljs.min.js"></script>
	<script src="~/js/devextreme/FileSaver.min.js"></script>
	<script src="~/js/devextreme/aspnet/dx.aspnet.mvc.js"></script>
	<script src="~/js/devextreme/aspnet/dx.aspnet.data.js"></script>
	<script src="~/js/site.js"></script>
</head>

<body>
	<div class="wrapper">
		<nav id="sidebar" class="sidebar js-sidebar">
			<div class="sidebar-content js-simplebar">
				<a class="sidebar-brand" href="index.html">
					<span class="align-middle title-sidebar">
						<img src="/images/logo-shlv.png" class="logo" style="width:140px;" />
					</span>
				</a>
				@await Component.InvokeAsync("vcNavbar")
			</div>
		</nav>

		<div class="main">
			<nav class="navbar navbar-expand navbar-light navbar-bg">
				<a class="sidebar-toggle js-sidebar-toggle">
					<i class="hamburger align-self-center"></i>
				</a>

				<div class="navbar-collapse collapse" style="position: relative;">
					<ul class="navbar-nav navbar-align" style="position: absolute;top:-27px;right:0;">
						<li class="nav-item">
							<div class="input-group input-group-language">
								<span class="input-group-text">
									<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-translate" viewBox="0 0 16 16">
