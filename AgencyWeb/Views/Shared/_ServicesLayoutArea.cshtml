</html>

<script>
	function onButtonAccountClicked() {
		location.href = '@Url.Action("Index", "PersonalInfo", new { area = "Account" })';
	}

	function onButtonDashboardClicked() {
		location.href = '@Url.Action("Index", "General", new { area = "Dashboard" })';
	}

	function onButtonSupportClicked() {
		location.href = '@Url.Action("Index", "General", new { area = "Dashboard" })';
	}

	function onButtonLogoutClicked() {
		location.href = '@Url.Action("Index", "Home", new { area = "" })';
	}
</script>